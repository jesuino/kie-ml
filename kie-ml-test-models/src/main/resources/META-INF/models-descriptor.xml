<models>
	<model>
		<id>mnist</id>
		<name>MNIST digit recognition</name>
		<descrtiption>Recognize handwritten digits using the MNIST dataset
		</descrtiption>
		<provider>deeplearning4j</provider>
		<modelBinPath>models/minist-model.zip</modelBinPath>
		<modelLabelsPath>labels/mnist_labels.txt</modelLabelsPath>
		<transformDescriptor>
			<name>ImageTransformer</name>
			<param>
				<name>height</name>
				<value>28</value>
			</param>
			<param>
				<name>width</name>
				<value>28</value>
			</param>
			<param>
				<name>channels</name>
				<value>1</value>
			</param>
		</transformDescriptor>
	</model>
